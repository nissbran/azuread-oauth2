{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "ClientId": "<app registration client id>",
    "TenantId": "<tenant id>",
    "Audience": "<app registration uri id>"
  },
  "Manual": {
    "Authority": "https://login.microsoftonline.com/<tenant id>/",
    "Audience": "<app registration uri id>"
  },
  "AllowedHosts": "*"
}
